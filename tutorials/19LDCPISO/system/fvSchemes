FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss midPoint;
    //grad(p)         Gauss linear;
}

divSchemes
{
    default         Gauss midPoint;
    //div(phi,U)      bounded Gauss midPoint;
    //div((interpolate(U)&S),U) bounded Gauss midPoint;
}

laplacianSchemes
{
    default         Gauss midPoint orthogonal;
}

interpolationSchemes
{
    default         midPoint;
}

snGradSchemes
{
    default         orthogonal;
}

fluxRequired
{
    default         no;
    p;
    Phi;
}


